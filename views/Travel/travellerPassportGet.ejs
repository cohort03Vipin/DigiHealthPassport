<html>
    <head>
        <title>Traveller</title>
        <!-- Bootstrap core CSS -->        
        <%- include('./partials/headscripts.ejs')%>        
    </head>
  <body>
    <%- include('./partials/navTraveller.ejs')%>
    <div class="container py-4">
        <br><br>
    
    <div id="trvlrPassport" class="py-3">
          <div class="col-md-10 mb-10 border rounded">
                  <h2 class="py-3">Traveller Passport Details</h2>
                  <br>               
                
                        <div class="row">
                            <div class="form-group col-md-6 mb-6">
                                <label for="tbxPassportNo">Passport No</label>
                                <label class="form-control"><%=passportNo %></label>
                                
                            </div>
                            <div class="form-group col-md-6 mb-6">
                                <label for="tbxPlaceOfBirth">Birth Place</label>
                                <label class="form-control"><%=placeofBirth %></label>
                            </div>
                        </div>
                        <div class="row">
                            <div class="form-group col-md-6 mb-6">
                                <label for="tbxValidityFrom">Passport Validity From</label>
                                <label class="form-control"><%=passportValidatyFrom %></label>
                             </div>
        
                            <div class="form-group col-md-6 mb-6">
                                <label  for="tbxValidityTo">Passport Validity To</label>
                                <label class="form-control"><%=passportValidatyTo %></label>
                            </div>
                        </div>
                        <div class="row"> 
                            <div class="form-group col-md-6 mb-6">
                                <label for="selectNationality">Nationality</label>
                                <label class="form-control"><%=nationality %> </label>
                            </div>
                             <div class="form-group col-md-6 mb-6">
                                <label for="tbxAuthority">Issuing Authority</label>
                                <label class="form-control"><%=issuingAuthority %> </label>
                             </div>
                        </div>
                       
                        <div class="form-group"> 
                            <label for="uploadPassport">Please click on the below link</label><br>                             
                            <a  href="https://ipfs.io/ipfs/<%=travelTicket%>"  target="_blank" ><%=passportCopy%></a>   
                        </div>
                        <div class="row mb-4">
                            <div class="col-md-10 mb-10 text-right">
                                <button type="button" class="btn btn-primary ">Back</button> 
                            </div> 
                        </div> 
                     
            </div> 
    </div>
    </div>
    <%- include('./partials/footer.ejs')%>
    <script type="text/javascript" src="javascripts/travellerCS.js"></script>
        </body>
  </html>